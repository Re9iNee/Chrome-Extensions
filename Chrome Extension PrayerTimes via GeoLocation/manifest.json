{
    "manifest_version": 2,
    "name": "Owghat",
    "version": "0.1",
    "browser_action": {
        "default_title": "Show Prayer Times",
        "default_popup": "popup.html",
        "default_icon": "/icons/128.png"
    },
    "icons": {
        "128": "/icons/128.png",
        "48": "/icons/48.png",
        "16": "/icons/16.png"
    },
    "permissions": [
        "geolocation"
    ]
}