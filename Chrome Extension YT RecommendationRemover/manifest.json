{
    "manifest_version": 2,
    "name": "Remove YoutubeRecommendations",
    "version": "0.1",
    "description": "My First Extenstion",
    "content_scripts": [{
        "matches": [
            "http://youtube.com/*",
            "https://www.youtube.com/*"
        ],
        "js": ["content.js"]
    }],
    "browser_action":{
        "default_title": "YouTube Recommendation Section Remover",
        "default_icon": "ct.png"
    },
    "icons":{
        "128": "ct.png",
        "48": "ct.png",
        "16": "ct.png"
    }
}